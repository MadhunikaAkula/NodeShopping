<%- include('../includes/head.ejs') %>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
<link rel="stylesheet" href="/css/categeory.css">

</head>

<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <div class="card">
            <div class="card-body">
                <button class="btn btn-success" ><a class="color" href="/config/addAdmin">Add Admin</a></button>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th scope="col">AdminName</th>
                            <th scope="col">AdminMail</th>
                            <th scope="col">AdminCompany</th>
                            <!-- <th scope="col">Action</th> -->
                        </tr>
                        
                    </thead>
                    <% for(var i=0; i < admins.length; i++) { %>
                        <tr>
                            <td><%= admins[i].username %></td>
                            <td><%= admins[i].email %></td>
                            <td><%= admins[i].companyname %></td>
                            <!-- <td><a href="/config/Editcategeory/<%= admins[i]._id %>?edit=true">Edit</a></td> -->
                        </tr>
                        <% } %>
                </table>
            </div>
        </div>
     
    </main>
    <%- include('../includes/end.ejs') %>